@model IEnumerable<Firma.Data.Data.Sklep.ItemType>
<div class="nav-wrapper container">
    <a id="logo-container" href="https://localhost:7120/" class="brand-logo" style="font-family: -apple-system">Shop</a>

    <ul class="right hide-on-med-and-down">
        <li>@Html.ActionLink("Strona Główna", "Index", "Home")</li>
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.Nazwa, "Index", "Shop", new { id = item.ItemTypeId }, null)</li>
        }

    </ul>

    <ul id="nav-mobile" class="sidenav">
        <li>@Html.ActionLink("Strona Główna", "Index", "Home")</li>
        @foreach (var item in Model)
        {
            <li>@Html.ActionLink(item.Nazwa, "Index", "Shop", new { id = item.ItemTypeId }, null)</li>
        }
    </ul>
    <a href="#" data-target="nav-mobile" class="sidenav-trigger">
        <i class="material-icons">menu</i>
    </a>

</div>